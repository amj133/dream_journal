<div class='contributors-list-container'>
  <div class='contributors-list'>
    <h3>List of contributors</h3>

    <% @users.each do |user| %>
      <%= link_to user.user_name, user_path(user) %><br>
    <% end %>
  </div>

  <div class="contributors-image-container">
    <%=image_tag("tree2.png", alt: "beautiful tree", id:"home-image") %>
  </div>
</div>
